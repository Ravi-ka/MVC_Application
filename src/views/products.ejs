
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                    <th scope="col">Action</th>
                    
                </tr>
            </thead>
            <tbody>
               <% products.forEach((product)=>{ %>
                    <tr>
                    <th><%= product.id %></th>
                    <td><%= product.name %></td>
                    <td><%= product.desc %></td>
                    <td scope="col"><%= product.price %></td>
                    <td>
                        <img src="<%= product.imageUrl %>" width="100px" alt="<%= product.name %>"/>
                    </td>
                    <td>
                        <a href="/update/<%= product.id %>" class="btn btn-warning">Update</a>
                        <button onclick="deleteFunction('<%= product.id %>')" class="btn btn-danger">Delete</button>
                        <!-- <a href="/delete/<%= product.id %>" class="btn btn-danger">Delete</a> -->
                    </td>
                </tr>
              <% }) %>
                
            </tbody>
        </table>
    